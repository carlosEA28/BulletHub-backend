{
  "name": "my-better-t-app",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "better-auth": "^1.3.28",
      "dotenv": "^17.2.2",
      "zod": "^4.1.11",
      "tsdown": "^0.15.5"
    }
  },
  "scripts": {
    "dev": "bun run --filter '*' dev",
    "build": "bun run --filter '*' build",
    "check-types": "bun run --filter '*' check-types",
    "dev:native": "bun run --filter native dev",
    "dev:web": "bun run --filter web dev",
    "dev:server": "bun run --filter server dev",
    "db:push": "bun run --filter @my-better-t-app/db db:push",
    "db:studio": "bun run --filter @my-better-t-app/db db:studio",
    "db:generate": "bun run --filter @my-better-t-app/db db:generate",
    "db:migrate": "bun run --filter @my-better-t-app/db db:migrate",
    "db:start": "bun run --filter @my-better-t-app/db db:start",
    "db:watch": "bun run --filter @my-better-t-app/db db:watch",
    "db:stop": "bun run --filter @my-better-t-app/db db:stop",
    "db:down": "bun run --filter @my-better-t-app/db db:down"
  },
  "packageManager": "bun@1.2.21",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.2",
    "multer": "^2.0.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "tsdown": "^0.15.5",
    "@types/bun": "^1.2.6"
  }
}
